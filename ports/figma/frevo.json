{
  "name": "Frevo",
  "version": "1.0.0",
  "description": "Frevo palette tokens for Figma (unchanged from theme/frevo-theme.yaml)",
  "tokens": {
    "color": {
      "bg": {
        "primary": { "$value": "#1A1F2E", "$type": "color" },
        "secondary": { "$value": "#21283A", "$type": "color" },
