{
  "$schema": "vscode://schemas/color-theme",
  "name": "Frevo",
  "type": "dark",
  "colors": {
    "editor.background": "#1A1F2E",
    "editor.foreground": "#F2F4F8",
    "editorLineNumber.foreground": "#4A6083",
    "editorLineNumber.activeForeground": "#F2F4F8",
    "editor.selectionBackground": "#2B4A7A",
    "editor.inactiveSelectionBackground": "#2B4A7A80",
    "editor.lineHighlightBackground": "#0F285180",
    "editor.findMatchHighlightBackground": "#C7A34080",
    "editorCursor.foreground": "#F2F4F8",

    "editorGroup.border": "#1F3D68",
    "editorGroupHeader.tabsBackground": "#21283A",
    "tab.activeBackground": "#2F3D5C",
    "tab.inactiveBackground": "#21283A",
    "tab.activeForeground": "#F2F4F8",
    "tab.inactiveForeground": "#B0B8CC",
    "tab.border": "#1F3D68",

    "sideBar.background": "#21283A",
    "sideBar.foreground": "#F2F4F8",
    "sideBarSectionHeader.background": "#21283A",
    "sideBarSectionHeader.foreground": "#F2F4F8",
    "activityBar.background": "#21283A",
    "activityBar.foreground": "#F2F4F8",
    "activityBarBadge.background": "#4B9AFF",
    "activityBarBadge.foreground": "#1A1F2E",

    "statusBar.background": "#1A1F2E",
    "statusBar.foreground": "#F2F4F8",
    "statusBarItem.remoteBackground": "#4B9AFF",
    "statusBarItem.remoteForeground": "#1A1F2E",

    "panel.background": "#21283A",
    "panel.border": "#1F3D68",

    "titleBar.activeBackground": "#1A1F2E",
    "titleBar.activeForeground": "#F2F4F8",
    "titleBar.inactiveBackground": "#1A1F2E",
    "titleBar.inactiveForeground": "#B0B8CC",

    "terminal.foreground": "#F2F4F8",
    "terminal.background": "#1A1F2E",
    "terminalCursor.foreground": "#F2F4F8",
    "terminal.selectionBackground": "#2B4A7A",

    "badge.background": "#4B9AFF",
    "badge.foreground": "#1A1F2E",

    "focusBorder": "#4B9AFF",
    "selection.background": "#2B4A7A",
    "dropdown.background": "#21283A",
    "dropdown.listBackground": "#21283A",
    "input.background": "#21283A",
    "input.foreground": "#F2F4F8",
    "input.border": "#1F3D68",
    "inputOption.activeBorder": "#4B9AFF",
    "inputValidation.infoBorder": "#00B7FF",
    "inputValidation.warningBorder": "#DAA445",
    "inputValidation.errorBorder": "#FF5B5F",

    "scrollbarSlider.background": "#1F3D68AA",
    "scrollbarSlider.hoverBackground": "#1F3D68CC",
    "scrollbarSlider.activeBackground": "#4B9AFFAA"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#7AD96A",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#00A3E5"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#FF6700"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#4B9AFF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#D966FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.function", "meta.function-call"],
      "settings": {
        "foreground": "#00A3E5"
      }
    },
    {
      "scope": ["entity.name.class", "entity.name.type"],
      "settings": {
        "foreground": "#F0B000",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["variable", "meta.definition.variable"],
      "settings": {
        "foreground": "#F2F4F8"
      }
    },
    {
      "scope": ["constant.language.boolean", "keyword.operator"],
      "settings": {
        "foreground": "#FF6700"
      }
    },
    {
      "scope": ["punctuation", "punctuation.separator", "punctuation.terminator"],
      "settings": {
        "foreground": "#B0B8CC"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#FF5B5F",
        "fontStyle": "bold"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "variable.readonly.defaultLibrary": {
      "foreground": "#B0B8CC"
    },
    "property": {
      "foreground": "#00A3E5"
    },
    "enumMember": {
      "foreground": "#F0B000"
    },
    "parameter": {
      "foreground": "#F2F4F8"
    }
  },
  "terminalColors": {
    "terminal.ansiBlack": "#0D2449",
    "terminal.ansiRed": "#FF5B5F",
    "terminal.ansiGreen": "#5FD94F",
    "terminal.ansiYellow": "#F0B000",
    "terminal.ansiBlue": "#4B9AFF",
    "terminal.ansiMagenta": "#D966FF",
    "terminal.ansiCyan": "#00A3E5",
    "terminal.ansiWhite": "#F2F4F8",
    "terminal.ansiBrightBlack": "#6B7589",
    "terminal.ansiBrightRed": "#FF8A8D",
    "terminal.ansiBrightGreen": "#8EFF7D",
    "terminal.ansiBrightYellow": "#FFE566",
    "terminal.ansiBrightBlue": "#7AB6FF",
    "terminal.ansiBrightMagenta": "#EA9BFF",
    "terminal.ansiBrightCyan": "#5DD4FF",
    "terminal.ansiBrightWhite": "#FFFFFF"
  }
}
